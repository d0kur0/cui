{
	"useTabs": true,
	"printWidth": 110,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "avoid",
	"importOrder": [
		"(.+)/pages/(.+)",
		"(.+)/components/(.*)$",
		"(.+)/hooks/(.*)$",
		"(.+)/stores/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
