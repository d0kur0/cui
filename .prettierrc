{
	"useTabs": true,
	"printWidth": 90,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "avoid",
	"importOrder": [
		"^\\.\\/pages/(.*)$",
		"^\\.\\/components/(.*)$",
		"^\\.\\/hooks/(.*)$",
		"^\\.\\/stores/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
